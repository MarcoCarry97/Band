GridBackground
{
	--grid-background:var(--ds-colors-grid-grey-1);
	--line-color:var(--ds-colors-grid-grey-2);
	--thick-line-color:var(--ds-colors-grid-grey-3);
	--spacing:var(--ds-colors-grid-25);
}

:root
{
	--ds-colors-grid-grey-1:#2b2b2b;
	--ds-colors-grid-grey-2:rgba(51,51,51,0.5);
	--ds-colors-grid-grey-3:rgba(51,51,51,1);

	--ds-colors-grid-25:25;
}